{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "app-web",
    "labels": {
      "app": "app",
      "proces-type": "web",
      "app-process-type": "app-web"
    }
  },
  "spec": {
    "replicas": "2",
    "selector": {
      "matchLabels": {
        "app": "app",
        "process-type": "web",
        "app-process-type": "app-web"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "app",
          "process-type": "web",
          "app-process-type": "app-web"
        }
      },
      "spec": {
        "containers": [
          {
            "args": [
              "web"
            ],
            "command": [
              "/start"
            ],
            "env": [],
            "image": "dokku/app",
            "name": "app-web",
            "ports": [
              {
                "containerPort": "5000"
              }
            ],
            "resources": {
              "limits": {
              },
              "requests": {
              }
            }
          }
        ],
        "revisionHistoryLimit": 5
      }
    }
  }
}
